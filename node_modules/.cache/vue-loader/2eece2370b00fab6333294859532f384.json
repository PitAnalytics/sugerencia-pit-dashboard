n      this.peps.forEach(item => {\n        total+=item.subtotal2019;\n      });\n      return total;\n    },\n    hidden(){\n      let hidden;\n      if(this.level===1){\n        hidden=false;\n      }\n      else{\n        hidden=true;\n      }\n      return hidden;\n    },\n    proveedorParam(){\n      return this.proveedor;\n    }\n  },\n  mounted(){\n\n  },\n  methods:{\n    ...Vuex.mapMutations(['setPeps','setPep','setPedidos','setLevel']),\n    ...Vuex.mapActions(['requestPeps','requestPedidos']),\n    loadPep(pep){\n      this.setLevel(2);\n      this.setPep(pep);\n      this.requestPedidos(pep);\n    },\n    textSearch(text){\n      this.requestPeps({proveedor:this.proveedor,text:text});\n    }\n  }\n}\n</script>\n<style scoped>\ninput{\n  border-radius:0px;\n}\n.btn{\n  border-radius:0px;\n  width: 100px;\n}\n</style>\n"]}]}